@*<div class="bg-primary panel-body"><h4>Create User</h4></div>*@
@*<div asp-validation-summary=" All" class="text-danger "></div>*@
<div>
    <div class="col-lg-10">
        <form asp-action="FormularioCrearParte" class="form-horizontal" method="post">
            <div class="text-danger" asp-validation-summary="All"></div>
            <div class="form-group">
                <label class="col-lg-2 ">Nombre</label>
                <div class="col-lg-3">
                    <input asp-for="Nombre" class="form-control" placeholder="Nombre" required />
                </div>
            </div>
            <div class="form-group">
                <label class="col-lg-2 ">Numero Parte</label>
                <div class="col-lg-3">
                    <input asp-for="NumeroParte" class="form-control" placeholder="Numero Parte" />
                </div>
            </div>
       
            <button type="submit" class="btn btn-primary">Crear Parte</button>
            <a asp-action="IndiceParte" asp-controller="Datos" class="btn btn-default">Cancelar</a>
        </form>
    </div>
    <div class="col-lg-2">

    </div>
</div>